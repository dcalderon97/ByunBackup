#include<iostream>
#include<cstdlib>
#include<cmath>
using namespace std;

int main()
{
	int n;
	cout << "Please enter the number to get all its primes" << endl;
	cin >> n;
	int A[userInput];
	int j = 0;
	for(int p = 2; p < userInput;p++)
		A[p] = p;
	for(int p = 2; p < floor(sqrt(userInput));p++)
		if(A != 0)
			j = p*p;
			while(j<=userInput)
			{
				A[j] = 0;
			}
	return EXIT_SUCCESS;
}




