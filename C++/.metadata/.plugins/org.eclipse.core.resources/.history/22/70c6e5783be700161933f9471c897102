#include<iostream>
#include<cstdlib>
#include<cmath>
using namespace std;

int main()
{
	int n;
	cout << "Please enter the number to get all its primes" << endl;
	cin >> n;

	return EXIT_SUCCESS;
}
int* sieve(int n)
{
	int A[n];
	int j = 0;
	for(int p = 2; p < n;p++)
		A[p] = p;
	for(int p = 2; p < floor(sqrt(n));p++)
	{
		if(A != 0)
			j = p*p;
			while(j<=n)
			{
				A[j] = 0;
				j = j+p;
			}
	}
	int i = 0;
	for(int p = 2; p < n;p++)
	{
		if(A[p] != 0)
			cout << A[p] << endl;
	}
}



