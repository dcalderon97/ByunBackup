import java.util.Arrays;

public class SortedString {
	static String[] sortList(String[] args)
	{
		String[] ara = new String[args.length];
		System.arraycopy(args, 0, ara, 0, args.length);
		Arrays.sort(ara);
		return ara;
	}
	public static void main(String[] args) {
		String ara1[] = new String[]{"APPLE","HELLO","AP"};
		sortList(ara1);
	}
}
