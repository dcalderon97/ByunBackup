package exercise2;

import java.io.Console;

public class DisplayName {

	public static void main(String[] args) {
		Console console = System.console();
		String firstName = console.readLine("Enter First name");
		String first  = "Daniel";
		String last = "Calderon";
		String id = "cald3384";
		String major = "CS";
		System.out.println("My name is: " + first + last);
		System.out.println("My id is: "+ id);
		System.out.println("My Major is: "+ major );
		System.out.println("My favorite song is: Dont know"  );

	}

}
