import java.util.Arrays;

public class SortedString {
	String[] sortList(String[] args)
	{
		String[] ara = new String[200];
		System.arraycopy(args, 0, ara, 0, args.length);
		Arrays.sort(ara);
		return ara;
	}
	public static void main(String[] args) {
		SortedString cla = new SortedString();
		String[] ara1 = new String[5];
		ara1[0] = "APPLE";
		ara1[1] = "JOHN";
		ara1[2] = "HELLO";
		ara1[3] = "BILL";
		ara1[4] = "NANCY";
		String[] sortAra = new String[5];
		System.arraycopy(cla.sortList(ara1),0,sortAra,0,ara1.length);

	}
}
