import java.util.Arrays;

public class SortedString {
	static void sortList(String[] args)
	{

		Arrays.sort(args);
	}
	public static void main(String[] args) {
		String ara1[] = new String[]{"APPLE","HELLO","APEL"};
		sortList(ara1);
		System.out.println(ara1[0]);
		System.out.println(ara1[1]);
		System.out.println(ara1[2]);
	}
}
